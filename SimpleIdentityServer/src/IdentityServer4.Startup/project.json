{
  "version": "1.0.0-*",
  "tooling": {
    "defaultNamespace": "IdentityServer4.Startup"
  },
  "packOptions": {
    "tags": [ "" ],
    "licenseUrl": "",
    "projectUrl": ""
  },

  "dependencies": {
    "Microsoft.Extensions.Configuration.CommandLine": "1.0.0",
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.0",
    "Microsoft.AspNetCore.Server.IISIntegration": "1.0.0",
    "Microsoft.AspNetCore.Mvc": "1.0.0",
    "Microsoft.AspNetCore.StaticFiles": "1.0.0",
    "Microsoft.AspNetCore.Authentication.Cookies": "1.0.0",
    "Microsoft.Extensions.Configuration.Json": "1.0.0",
    "Microsoft.Extensions.Logging.Console": "1.0.0",
    "Microsoft.EntityFrameworkCore.SqlServer": "1.0.0",
    "Microsoft.EntityFrameworkCore.Tools": {
      "type": "build",
      "version": "1.0.0-preview2-final"
    },
    "Microsoft.AspNetCore.Server.Kestrel.Https": "1.0.0",
    "Serilog.Extensions.Logging": "1.0.0",
    "Serilog.Sinks.RollingFile": "2.0.0",
    "IdentityServer4": "1.0.0-rc1",
    "Serilog.Sinks.ColoredConsole": "2.0.0",
    "Microsoft.AspNetCore.Diagnostics": "1.0.0",
    "Microsoft.Extensions.Logging.Debug": "1.0.0",
    "SimpleIdentityServer.IdentityServer.EF": { "target": "project" },
    "SimpleIdentityServer.Authentication.Middleware": { "target": "project" },
    "WebApiContrib.Core.Storage.Redis": { "target": "project" }
  },
  "tools": {
    "Microsoft.EntityFrameworkCore.Tools": {
      "imports": [ "dnxcore50", "portable-net451+win8" ],
      "version": "1.0.0-preview2-final"
    }
  },
  "frameworks": {
    "netcoreapp1.0": {
      "imports": [ "dotnet5.6", "dnxcore50", "portable-net451+win8" ],
      "dependencies": {
        "System.Threading.Tasks.Extensions": "4.0.0"
      }
    }
  },
  "compilationOptions": {
	"emitEntryPoint": true,
	"debugType": "portable"
  },
  "buildOptions": {
    "emitEntryPoint": true,
    "preserveCompilationContext": true,
    "copyToOutput": [
      "appsettings*.json",
      "SimpleIdServer.pfx"
    ]
  },
  "publishOptions": {
    "include": [
      "appsettings*.json",
      "SimpleIdServer.pfx"
    ]
  },
  "runtimes": {
    "win10-x64": { },
    "win7-x64": { }
  }
}
