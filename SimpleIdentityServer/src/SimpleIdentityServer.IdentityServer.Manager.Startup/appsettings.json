{
  "Log": {
    "File": {
      "Enabled": "true",
      "PathFormat": "log-{Date}.txt"
    },
    "Elasticsearch": {
      "Enabled": "true",
      "Url": "http://localhost:9200"
    }
  },
  "Caching": {
    "Database": "INMEMORY"
  },
