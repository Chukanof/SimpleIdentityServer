{
  "Log": {
    "File": {
      "Enabled": "true",
      "PathFormat": "log-{Date}.txt"
    },
    "Elasticsearch": {
      "Enabled": "true",
      "Url": "http://localhost:9200"
    }
  },
  "Db": {
    "Type": "INMEMORY",
    "ConnectionString": "Data Source=.;Initial Catalog=SimpleIdentityServerConfiguration;Integrated Security=True;",
    "IdServerConnectionString": "Data Source=.;Initial Catalog=IdentityServer;Integrated Security=True;"
  },
  "Caching": {
    "Type": "INMEMORY",
    "ConnectionString": "localhost",
    "InstanceName": "ConfigurationInstance",
    "Port": "6379"
  },
  "IntrospectionEdp": {
    "Url": "https://localhost:5443/connect/introspect",
    "ClientId": "configuration",
    "ClientSecret": "configuration"
  }
}
