{
  "Log": {
    "File": {
      "Enabled": "true",
      "PathFormat": "log-{Date}.txt"
    },
    "Elasticsearch": {
      "Enabled": "true",
      "Url": "http://localhost:9200"
    }
  },
  "Caching": {
    "Type": "INMEMORY"
  },
  "OpenId": {
    "IntrospectUrl": "http://localhost:5000/introspect",
    "ClientId": "OpenIdManager",
    "ClientSecret": "OpenIdManager"
  },
  "Db": {
    "Type": "INMEMORY",
    "ConnectionString": "Data Source=.;Initial Catalog=SimpleIdentityServer;Integrated Security=True;"
  }
}
