{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Verbose",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Log": {
    "File": {
      "Enabled": "true",
      "PathFormat": "log-{Date}.txt"
    },
    "Elasticsearch": {
      "Enabled": "true",
      "Url": "http://localhost:9200"
    }
  },
  "Caching": {
    "Database": "INMEMORY"
  },
  "AuthorizationServer": "https://localhost:5443",
  "ClientId": "OpenIdManager",
  "ClientSecret": "OpenIdManager",
  "DatabaseType": "INMEMORY"
}
