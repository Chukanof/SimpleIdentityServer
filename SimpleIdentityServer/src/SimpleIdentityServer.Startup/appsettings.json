{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Verbose",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Data": {
    "DefaultConnection": {
      "ConnectionString": "Data Source=.;Initial Catalog=SimpleIdentityServer;Integrated Security=True;"
    }
  },
  "EntityFramework": {
    "ApplicationDbContext": {
      "ConnectionStringKey": "Data:DefaultConnection:ConnectionString"
    }
  },
  "Log": {
    "File": {
      "Enabled": "true",
      "PathFormat": "log-{Date}.txt"
    },
    "Elasticsearch": {
      "Enabled": "true",
      "Url": "http://localhost:9200"
    }
  },
  "isSqlServer": "true",
  "isSqlLite": "false",
  "isPostgre": "false",
  "ClientId": "SimpleIdentityServer",
  "ClientSecret": "SimpleIdentityServer",
  "ConfigurationUrl": "http://localhost:5004/configuration",
  "AuthServerUrl": "http://localhost:5000/.well-known/openid-configuration"
}
