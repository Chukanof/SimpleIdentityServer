    "SimpleIdentityServer.Configuration.Core": "*"
  },
  "tools": {
    "Microsoft.EntityFrameworkCore.Tools": {
      "imports": [ "dnxcore50", "portable-net451+win8" ],
      "version": "1.0.0-preview2-final"
    }
  },
  "frameworks": {
    "net46": { },
    "netcoreapp1.0": {
