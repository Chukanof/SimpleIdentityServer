{
  "version": "1.0.0-*",
  "tooling": {
    "defaultNamespace": "SimpleIdentityServer.Startup"
  },
  "packOptions": {
    "tags": [ "" ],
    "licenseUrl": "",
    "projectUrl": ""
  },

  "dependencies": {
    "Microsoft.Extensions.Configuration.CommandLine": "1.0.0-rc2-final",
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.0-rc2-final",
    "Microsoft.AspNetCore.Server.Kestrel.Https": "1.0.0-rc2-final",
    "Microsoft.AspNetCore.Server.IISIntegration": "1.0.0-rc2-final",
    "SimpleIdentityServer.Host": "*"
  },
  "frameworks": {
    "net46": {
      "frameworkAssemblies": {
        "System.Web": "4.0.0.0"
      }
    },
    "netcoreapp1.0": {
      "imports": [ "dotnet5.6", "dnxcore50", "portable-net451+win8" ],
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0-rc2-3002702"
        },
        "System.Threading.Tasks.Extensions": "4.0.0-rc2-24027"
      }
    }
  },
  "buildOptions": {
    "emitEntryPoint": true,
    "preserveCompilationContext": true,
    "copyToOutput": [
      "appsettings.json",
      "appsettings.azure.json",
      "appsettings.docker.json",
      "SimpleIdServer.pfx"
    ]
  },
  "publishOptions": {
    "include": [
      "appsettings.json",
      "appsettings.azure.json",
      "appsettings.docker.json",
      "SimpleIdServer.pfx"
    ]
  }
}
