{
  "version": "1.0.0-*",
  "authors": [ "Brock Allen", "Dominick Baier", "Scott Brady" ],
  "description": "EntityFramework persistence layer for IdentityServer4",

  "packOptions": {
    "tags": [ "OAuth2", "OAuth 2.0", "OpenID Connect", "Security", "Identity", "IdentityServer", "EntityFramework" ],
    "projectUrl": "https://github.com/IdentityServer/IdentityServer4.EntityFramework",
    "licenseUrl": "https://github.com/IdentityServer/IdentityServer4.EntityFramework/LICENSE",
    "iconUrl": "https://identityserver.github.io/Documentation/assets/images/icons/IDserver_icon128.jpg"
  },

  "dependencies": {
    "IdentityServer4": "1.0.0-rc1",
    "Microsoft.EntityFrameworkCore": "1.0.0",
    "Microsoft.EntityFrameworkCore.Relational": "1.0.0",
    "Microsoft.EntityFrameworkCore.SqlServer": "1.0.0",
    "Microsoft.EntityFrameworkCore.Tools": {
      "type": "build",
      "version": "1.0.0-preview2-final"
    },
    "Microsoft.Extensions.Configuration.Json": "1.0.0",
    "Microsoft.Extensions.Configuration.EnvironmentVariables": "1.0.0",
    "IdentityServer4.EntityFramework": {
      "target": "project"
    },
    "SimpleIdentityServer.Core": "*"
  },

  "tools": {
    "Microsoft.EntityFrameworkCore.Tools": {
      "imports": [ "dnxcore50", "portable-net451+win8" ],
      "version": "1.0.0-preview2-final"
    }
  },

  "buildOptions": {
    "emitEntryPoint": true,
    "copyToOutput": [
      "appsettings.json"
    ]
  },
   
  "frameworks": {
      "net46": { },
      "netcoreapp1.0": {
        "imports": [ "dnxcore50", "portable-net451+win8" ],
        "dependencies": {
          "Microsoft.NETCore.App": {
            "type": "platform",
            "version": "1.0.0"
          }
        }
      }
    }
  }
